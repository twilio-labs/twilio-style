{
  "name": "twilio-style",
  "version": "1.3.0",
  "description": "Twilio's JavaScript linting style",
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "styleguide",
    "twilio"
  ],
  "homepage": "https://github.com/twilio-labs/twilio-style#readme",
  "bugs": {
    "url": "https://github.com/twilio-labs/twilio-style/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilio-labs/twilio-style.git"
  },
  "license": "MIT",
  "author": "Twilio @twilio",
  "contributors": [
    "Alice Cai <ahcai@berkeley.edu>",
    "Kousha Talebian <k@ousha.me>"
  ],
  "scripts": {
    "audit": "npm audit && lerna-audit --no-fix",
    "audit:fix": "npm audit fix && lerna-audit",
    "postaudit:fix": "npm install",
    "bootstrap": "lerna exec -- npm i && lerna link",
    "postinstall": "npm run bootstrap",
    "lint": "lerna run lint --stream --no-prefix",
    "lint:fix": "lerna run lint:fix --stream --no-prefix",
    "publish": "lerna-travis-publish",
    "release:alpha": "lerna-travis-release alpha",
    "release:beta": "lerna-travis-release beta",
    "release:public": "lerna-travis-release public",
    "test": "lerna run test --stream --no-prefix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ultra lint",
      "pre-push": "ultra test"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@k88/lerna-travis": "^1.1.2",
    "@types/jest": "^26.0.19",
    "babel-jest": "^26.6.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lerna-audit": "^1.3.0",
    "ncu": "^0.2.1",
    "ultra-runner": "^3.7.1"
  }
}
